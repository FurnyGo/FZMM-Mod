  "authors": [
    "Zailer43",
    "DNTranslator"
  ],
  "contact": {
    "issues": "https://github.com/Zailer43/FZMM-Mod/issues",
    "sources": "https://github.com/Zailer43/FZMM-Mod"
  },
  "license": "Unlicense",
  "icon": "assets/fzmm/icon.png",
